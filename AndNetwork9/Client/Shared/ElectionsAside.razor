@using System.Globalization
            @if (Election is not null)
            {
                <b>Начало регистрации: </b>
                @Election.RegistrationDate.ToString("dd.MM.yyyy")
                <br/>
                <b>Начало голосования: </b>
                @Election.VoteDate.ToString("dd.MM.yyyy")
                <br/>
                <b>Конец голосования: </b>
                @Election.AnnouncementDate.ToString("dd.MM.yyyy")
                <br/>
                <b>Начало полномочий: </b>
                @Election.StartDate.ToString("dd.MM.yyyy")
                <br/>
                <b>Конец полномочий: </b>
                @Election.EndDate.ToString("dd.MM.yyyy")
                <br/>
            }
